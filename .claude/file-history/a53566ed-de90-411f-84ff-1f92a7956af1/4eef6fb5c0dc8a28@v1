# Conta registros antes
cursor.execute("SELECT COUNT(*) FROM copilot_cross_sell")
antes = cursor.fetchone()[0]

# Insere regras
sql = """
INSERT INTO copilot_cross_sell
(gatilho_tipo, gatilho_valor, sugestao_tipo, sugestao_valor, sugestao_nome, prioridade, mensagem_vendedor, ativo)
VALUES (%s, %s, %s, %s, %s, %s, %s, 1)
"""

cursor.executemany(sql, regras)
conn.commit()

# Conta depois
cursor.execute("SELECT COUNT(*) FROM copilot_cross_sell")
depois = cursor.fetchone()[0]

print(f"Registros antes: {antes}")
print(f"Registros adicionados: {depois - antes}")
print(f"Total agora: {depois}")

# Estatísticas por tipo
cursor.execute("SELECT gatilho_tipo, COUNT(*) FROM copilot_cross_sell GROUP BY gatilho_tipo ORDER BY COUNT(*) DESC")
print("\nPor tipo de gatilho:")
for row in cursor.fetchall():
    print(f"  - {row[0]}: {row[1]}")

cursor.execute("SELECT COUNT(*) FROM copilot_cross_sell WHERE prioridade >= 85")
print(f"\nAlta prioridade (>=85): {cursor.fetchone()[0]} regras")

cursor.close()
conn.close()
